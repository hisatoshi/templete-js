{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "plugins" : ["react"],
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion" : 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
        "indent": [
            "error",
            2
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "single"
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console" : [
          "off"
        ],
        "react/jsx-uses-vars": 1
    }
}
